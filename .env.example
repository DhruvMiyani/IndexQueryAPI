# Environment variables for Vector Database API

# Cohere API key for text embeddings (get from https://cohere.com/embeddings)
COHERE_API_KEY=your_cohere_api_key_here

# Application settings
PYTHONPATH=/app/src
PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1

# Optional: Database persistence settings
PERSISTENCE_ENABLED=true
PERSISTENCE_DIR=/app/data
SNAPSHOT_INTERVAL_MINUTES=30
WAL_ENABLED=true

# Optional: Distributed settings
ENABLE_DISTRIBUTED=false
NODE_ID=node-1
CLUSTER_NODES=node-2:8001,node-3:8002